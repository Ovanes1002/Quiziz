{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "camelcase" : true,
	"indent": 2,
	"undef": true,
	"quotmark": false,
	"maxlen": 120,
	"trailing": true,
	"curly": true,
	"strict": false,
	"browser": true,
	"devel": true,
	"jquery": true,
    "esversion": 9,
    "node": true
}
